#!/usr/bin/env python3
import os
import sys

sys.path.append(os.path.join(os.path.dirname(__file__), ".."))
sys.path.append('storybuilder')


def main():
    # application initialize
    from storybuilder.application import Application
    app = Application()

    # run application
    return app.run()


if __name__ == '__main__':
    sys.exit(main())
